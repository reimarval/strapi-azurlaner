{
  "kind": "collectionType",
  "collectionName": "pages",
  "info": {
    "singularName": "page",
    "pluralName": "pages",
    "displayName": "page",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "string"
    },
    "blocks": {
      "type": "dynamiczone",
      "components": [
        "blocks.recent-articles",
        "blocks.hero-text"
      ]
    },
    "publish_at": {
      "type": "datetime"
    },
    "ready": {
      "type": "boolean",
      "default": true,
      "required": true
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "shared.seo"
    },
    "tierCompact": {
      "type": "dynamiczone",
      "components": [
        "blocks.regular-title",
        "blocks.compact-tier"
      ]
    }
  }
}
